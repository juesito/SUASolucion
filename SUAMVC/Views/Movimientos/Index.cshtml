@model IEnumerable<SUADATOS.Movimiento>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Menu {
    <div>Movimientos <img src="~/Content/Images/people-b.png" width="15" height="15" /></div>
}

<table class="table table-striped" style="font-size:10px; width:100%;">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.lote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaTransaccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombreArchivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Asegurado.numeroAfiliacion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.lote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fechaTransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nombreArchivo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asegurado.numeroAfiliacion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
