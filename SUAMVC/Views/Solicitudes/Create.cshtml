@model SUADATOS.Solicitud

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Menu {
    <div>Alta de solicitudes <img src="~/Content/Images/people-b.png" width="15" height="15" /></div>
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="panel panel-info">
        <div class="panel-heading">
            <h3 class="panel-title">Captura de Solicitudes</h3>
        </div>
        <div class="panel-body">
            @Html.ValidationSummary(true, "", new { @class = "text-danger" })

            <table style="font-size:10px;margin:0 auto;">
                <tr>
                    <td>@Html.Label(@Language.cliente, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("clienteId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.clienteId, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.Label(@Language.proyecto, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("proyectoId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.proyectoId, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.Label(@Language.residencia, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("residenciaId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.residenciaId, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.LabelFor(model => model.fechaSolicitud, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.fechaSolicitud, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.fechaSolicitud, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.esquemaId, "esquemaId", htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("esquemaId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.esquemaId, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.LabelFor(model => model.sdiId, "sdiId", htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("sdiId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.sdiId, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.contratoId, "contratoId", htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("contratoId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.contratoId, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.LabelFor(model => model.fechaInicial, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.fechaInicial, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.fechaInicial, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.fechaFinal, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.fechaFinal, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.fechaFinal, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.LabelFor(model => model.tipoPersonalId, "tipoPersonalId", htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.DropDownList("tipoPersonalId", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.tipoPersonalId, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.solicita, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.solicita, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.solicita, "", new { @class = "text-danger" })
                    </td>
                    <td>@Html.LabelFor(model => model.valida, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.valida, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.valida, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.autoriza, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td>
                        @Html.EditorFor(model => model.autoriza, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.autoriza, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td>@Html.LabelFor(model => model.observaciones, htmlAttributes: new { @class = "control-label col-md-2" })</td>
                    <td colspan="3">
                        @Html.TextAreaFor(model => model.observaciones, new { htmlAttributes = new { @class = "form-control", @columns = "40" } })
                        @Html.ValidationMessageFor(model => model.observaciones, "", new { @class = "text-danger" })
                    </td>
                </tr>
                <tr>
                    <td colspan="3"></td>
                    <td style="text-align:right;"><input type="submit" value="Create" class="btn btn-default" /></td>
                </tr>

            </table>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
